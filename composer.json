{
    "name": "atelierspierrot/git-api",
    "type": "library",
    "description": "A PHP API to get infos and manage a GIT distant or local repository",
    "keywords": [ "VCS", "GIT" ],
    "homepage": "http://github.com/atelierspierrot/git-api",
    "license": "GPL-3.0",
    "authors": [{
        "name": "Les Ateliers Pierrot & contributors",
        "email": "contact@ateliers-pierrot.fr",
        "homepage": "http://www.ateliers-pierrot.fr/"
    },{
        "name": "Pierre Cassat",
        "email": "piwi@ateliers-pierrot.fr",
        "homepage": "http://github.com/pierowbmstr"
    }],
    "require": {
        "php": ">=5.3.0",
        "atelierspierrot/patterns": "1.*",
        "atelierspierrot/library": "1.*"
    },
    "require-dev": {
        "sami/sami": "dev-master"
    },
    "autoload": { "psr-0": {"GitApi": "src"} },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
